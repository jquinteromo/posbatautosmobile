<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3">


    <LinearLayout
    android:orientation="vertical"
    android:layout_width="60dp"
    android:layout_height="match_parent"
    android:background="#c9c9c9"
    >
        <ImageView
            android:layout_marginVertical="68dp"
            android:layout_width="50dp"
            android:layout_height="33dp"
            android:src="@drawable/search"></ImageView>

        <ImageView
            android:layout_marginVertical="1dp"
            android:layout_width="50dp"
            android:layout_height="33dp"
            android:src="@drawable/battery"></ImageView>

        <ImageView
            android:layout_marginVertical="60dp"
            android:layout_width="50dp"
            android:layout_height="33dp"
            android:src="@drawable/buildings"></ImageView>

        <ImageView

            android:layout_width="50dp"
            android:layout_height="33dp"
            android:src="@drawable/catalogo"></ImageView>

        <ImageView
            android:layout_marginVertical="60dp"
            android:layout_width="50dp"
            android:layout_height="33dp"
            android:src="@drawable/car"></ImageView>

        <ImageView
            android:layout_width="50dp"
            android:layout_height="33dp"
            android:src="@drawable/home"></ImageView>

        <ImageView
            android:layout_marginTop="45dp"
            android:layout_width="50dp"
            android:layout_height="33dp"
            android:src="@drawable/document"></ImageView>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_marginHorizontal="10dp"
        android:orientation="horizontal">


        <ImageView
            android:layout_marginLeft="45dp"
            android:layout_width="67dp"
            android:layout_height="100dp"
            android:src="@drawable/ic_channel_foreground"></ImageView>

        <TextView
            android:layout_width="46dp"
            android:layout_height="30dp"
            android:text="Inicio"
            android:layout_marginVertical="40dp"
            android:textSize="14dp"
            ></TextView>

        <TextView
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:text="Contacto"
            android:layout_marginVertical="40dp"
            android:textSize="14dp"
            ></TextView>

        <TextView
            android:layout_width="115dp"
            android:layout_height="30dp"
            android:text="Sobre Nosotros"
            android:layout_marginVertical="40dp"
            android:textSize="14dp"
            ></TextView>


        <ImageView
            android:id="@+id/show_shoppingcar"
            android:layout_marginVertical="30dp"
            android:layout_width="60dp"
            android:layout_height="36dp"
            android:src="@drawable/shoopig"></ImageView>



    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_marginHorizontal="70dp"
        android:layout_marginVertical="90dp"
        android:layout_width="330dp"
        android:layout_height="600dp"

        >



        <LinearLayout
            android:orientation="vertical"
            android:layout_width="120dp"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_marginTop="160dp"
                android:layout_marginRight="17dp"
                android:background="#d3d3d3"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="110dp">
                <ImageView
                    android:id="@+id/imageview"
                    android:layout_width="100dp"
                    android:layout_height="120dp"
                    android:src="@drawable/vartablu"></ImageView>

            </LinearLayout>


            <TextView
                android:id="@+id/reference_product"
                android:textStyle="bold"
                android:textSize="13dp"
                android:layout_marginTop="20dp"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:text="Batería varta blue
Lstv4"></TextView>

            <TextView
                android:id="@+id/priceProduct"
                android:textStyle="bold"
                android:textSize="13dp"
                android:layout_marginVertical="10dp"
                android:layout_width="60dp"
                android:layout_height="20dp"
                android:text="200.499"></TextView>

            <TextView
                android:textColor="#72cb10"
                android:textSize="11dp"
                android:layout_marginVertical="10dp"
                android:layout_width="160dp"
                android:layout_height="20dp"
                android:text="10% Descuento"></TextView>


            <Button
                android:id="@+id/add_car"
                android:textSize="8dp"
                android:text="Añadir al carrito"
                android:layout_width="100dp"
                android:layout_height="35dp"></Button>

        </LinearLayout>


        <LinearLayout
            android:orientation="vertical"
            android:layout_width="120dp"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_marginTop="160dp"
                android:layout_marginRight="17dp"
                android:background="#d3d3d3"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="110dp">
                <ImageView
                    android:layout_width="105dp"
                    android:layout_height="125dp"
                    android:src="@drawable/vartawhit"></ImageView>
            </LinearLayout>


            <TextView
                android:textStyle="bold"
                android:textSize="13dp"
                android:layout_marginTop="20dp"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:text="Batería varta Silver
Dinamic"></TextView>

            <TextView
                android:textStyle="bold"
                android:textSize="13dp"
                android:layout_marginVertical="10dp"
                android:layout_width="60dp"
                android:layout_height="20dp"
                android:text="$230.499"></TextView>

            <TextView
                android:textColor="#72cb10"
                android:textSize="11dp"
                android:layout_marginVertical="10dp"
                android:layout_width="160dp"
                android:layout_height="20dp"
                android:text="10% Descuento"></TextView>


            <Button

                android:id="@+id/add_car2"
                android:textSize="8dp"
                android:text="Añadir al carrito"
                android:layout_width="100dp"
                android:layout_height="35dp"></Button>

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="120dp"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_marginTop="160dp"
                android:layout_marginRight="17dp"
                android:background="#d3d3d3"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="110dp">
                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="120dp"
                    android:src="@drawable/vartablac"></ImageView>
            </LinearLayout>


            <TextView
                android:textStyle="bold"
                android:textSize="13dp"
                android:layout_marginTop="20dp"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:text="Batería varta F5
Black"></TextView>

            <TextView
                android:textStyle="bold"
                android:textSize="13dp"
                android:layout_marginVertical="10dp"
                android:layout_width="60dp"
                android:layout_height="20dp"
                android:text="$250.599"></TextView>

            <TextView
                android:textColor="#72cb10"
                android:textSize="11dp"
                android:layout_marginVertical="10dp"
                android:layout_width="160dp"
                android:layout_height="20dp"
                android:text="10% Descuento"></TextView>


            <Button

                android:id="@+id/add_car3"
                android:textSize="8dp"
                android:text="Añadir al carrito"
                android:layout_width="100dp"
                android:layout_height="35dp"></Button>

        </LinearLayout>

    </LinearLayout>




</androidx.coordinatorlayout.widget.CoordinatorLayout>